# ActingPrimary: TRUE					# BOOLEAN (Acting as primary ObjectServer)
# AlertSecurityModel: 0 				# INTEGER (Desktop security model)
# AllowConnections: TRUE 				# BOOLEAN (Specifies whether or not non-root users can connect)
# AllowISQL: TRUE 					# BOOLEAN (Specifies whether or not isql connections are allowed)
# AllowISQLWrite: TRUE 					# BOOLEAN (Specifies whether or not modifications by isql connections are allowed)
# AllowTimedRefresh: FALSE 				# BOOLEAN (Allow desktops to apply timed refresh)
