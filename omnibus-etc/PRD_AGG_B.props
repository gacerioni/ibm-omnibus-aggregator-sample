# ProfileStatsInterval: 60 				# INTEGER (Profiler statistics report interval)
# Props.CheckNames: TRUE 				# BOOLEAN (Cause program to abort if any property is not understood at the time it is read)
# PropsFile: '$OMNIHOME/etc/PRD_AGG_P.props' 		# STRING (Path to the property file.)
# RegexpLibrary: 'NETCOOL' 				# STRING (The name of the internal regexp library to use.)
# RestrictionUpdateCheck: TRUE 				# BOOLEAN (Specifies whether or not users can update events that will not appear in their view)
# RestrictPasswords: FALSE 				# BOOLEAN (Restrict use of characters in a password)
# RestrictionFiltersAND: TRUE 				# BOOLEAN (Specifies whether or not restriction filters are being AND'ed or 
# RestrictProxySQL: FALSE 				# BOOLEAN (Restrict the set of SQL commands proxies can submit)
# Sec.AuditLevel: 'warn' 				# STRING (Security audit level)
# Sec.AuditLog: '$OMNIHOME/log/NCOMS_audit_file.log' 	# STRING (Security audit trail)
