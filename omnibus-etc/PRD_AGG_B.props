# Sec.AuditSqlLog: '$OMNIHOME/log/NCOMS_audit_sql.log' 	# STRING (Security SQL command audit trail)
# SecureMode: FALSE 					# BOOLEAN (Secure authentication)
# Store.LocalizedSort: FALSE 				# BOOLEAN (Sort based on client locale.)
# Store.LocalizedSortCaseSensitive: 'OFF' 		# STRING (Localized sort case insensitively:OFF; lowercase first:LOWERFIRST; uppercase first:UPPERFIRST.)
# UniqueLog: FALSE 					# BOOLEAN (Make log file name unique (adds PID))

ActingPrimary: TRUE
AlertSecurityModel: 0
AllowConnections: TRUE
AllowISQL: TRUE
AllowISQLWrite: TRUE
AllowTimedRefresh: FALSE
Auto.Debug: FALSE
Auto.Enabled: TRUE
Auto.StatsInterval: 60
BackupObjectServer: FALSE
ClientHeartbeatDisable: FALSE
ClientHeartbeatRate: 10
ConfigCryptoAlg: 'AES'
ConfigKeyFile: ''
Connections: 256
DeleteLogging: FALSE
DeleteLogLevel: 0
DeleteLogSize: 1048576
DTMaxTopRows: 100
Granularity: 60
GWDeduplication: 0
Iduc.ListeningHostname: ''
Iduc.ListeningPort: 0
MaxLogFileSize: 1024
MessageLevel: 'debug'
NHttpd.AuthenticationDomain: 'omnibus'
NHttpd.EnableFileServing: FALSE
NHttpd.EnableHTTP: FALSE
NHttpd.ListeningPort: 0
NHttpd.SSLEnable: FALSE
NHttpd.SSLListeningPort: 0
NHttpd.SSLCertificate: ''
NHttpd.ExpireTimeout: 15
NHttpd.NumWorkThreads: 5
NRestOS.Enable: FALSE	
NRestOS.OSLCRDFMsgFormat: 'STANDARD'
PA.Name: 'OMNIBUSAGG_PA'
PA.Password: ''
PA.Username: 'root'
PasswordEncryption: 'DES'
PasswordFormat: '8:1:1:0:1'
Profile: TRUE
ProfileStatsInterval: 60
Props.CheckNames: TRUE
RegexpLibrary: 'NETCOOL'
RestrictionUpdateCheck: TRUE
RestrictPasswords: FALSE
RestrictProxySQL: FALSE
RestrictionFiltersAND: TRUE
Sec.AuditLevel: 'warn'
Sec.AuditGroups: 'System, Administrator'
SecureMode: FALSE
Store.LocalizedSort: FALSE
Store.LocalizedSortCaseSensitive: 'OFF'
UniqueLog: FALSE

MessageLevel: 'debug'                                   # STRING (Message reporting level)
MessageLog: '$OMNIHOME/log/PRD_AGG_B.log'               # STRING (Path to the message log file.)
Name: 'PRD_AGG_B'    
PropsFile: '$OMNIHOME/etc/PRD_AGG_B.props'


